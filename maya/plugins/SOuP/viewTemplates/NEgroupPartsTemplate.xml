<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEgroupParts'>
		<attribute name='outputGeometry' type='maya.geometry'>
			<label>Output Geometry</label>
		</attribute>
		<attribute name='inputGeometry' type='maya.geometry'>
			<label>Input Geometry</label>
		</attribute>
		<attribute name='inputComponents' type='maya.componentList'>
			<label>Input Components</label>
		</attribute>
		<attribute name='inputRemoveComponent' type='maya.componentList'>
			<label>Input Remove Component</label>
		</attribute>
		<attribute name='groupId' type='maya.long'>
			<label>Group Id</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEgroupParts'>
		<property name='outputGeometry'/>
		<property name='inputGeometry'/>
		<property name='inputComponents'/>
		<property name='inputRemoveComponent'/>
		<property name='groupId'/>
	</view>
</templates>