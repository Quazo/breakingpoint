<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEcocoon'>
		<attribute name='outMesh' type='maya.mesh'>
			<label>Out Mesh</label>
		</attribute>
		<attribute name='outPositionPP' type='maya.vectorArray'>
			<label>Out Position PP</label>
		</attribute>
		<attribute name='outRgbaPP' type='maya.doubleArray'>
			<label>Out Rgba PP</label>
		</attribute>
		<attribute name='outThicknessPP' type='maya.doubleArray'>
			<label>Out Thickness PP</label>
		</attribute>
		<attribute name='inPositionPP' type='maya.vectorArray'>
			<label>In Position PP</label>
		</attribute>
		<attribute name='inRgbaPP' type='maya.vectorArray'>
			<label>In Rgba PP</label>
		</attribute>
		<attribute name='inThicknessPP' type='maya.doubleArray'>
			<label>In Thickness PP</label>
		</attribute>
		<attribute name='visibility' type='maya.bool'>
			<label>Visibility</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEcocoon'>
		<property name='outMesh'/>
		<property name='outPositionPP'/>
		<property name='outRgbaPP'/>
		<property name='outThicknessPP'/>
		<property name='inPositionPP'/>
		<property name='inRgbaPP'/>
		<property name='inThicknessPP'/>
		<property name='visibility'/>
	</view>
</templates>