<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEfluidShape'>
		<attribute name='matrix' type='maya.matrix'>
			<label>Matrix</label>
		</attribute>
		<attribute name='inverseMatrix' type='maya.matrix'>
			<label>Inverse Matrix</label>
		</attribute>
		<attribute name='worldMatrix' type='maya.matrix'>
			<label>World Matrix</label>
		</attribute>
		<attribute name='worldInverseMatrix' type='maya.matrix'>
			<label>World Inverse Matrix</label>
		</attribute>
		<attribute name='parentMatrix' type='maya.matrix'>
			<label>Parent Matrix</label>
		</attribute>
		<attribute name='parentInverseMatrix' type='maya.matrix'>
			<label>Parent Inverse Matrix</label>
		</attribute>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
		<attribute name='outMatteOpacity' type='maya.float3'>
			<label>Out Matte Opacity</label>
		</attribute>
		<attribute name='outTransparency' type='maya.float3'>
			<label>Out Transparency</label>
		</attribute>
		<attribute name='outMesh' type='maya.mesh'>
			<label>Out Mesh</label>
		</attribute>
		<attribute name='currentTime' type='maya.time'>
			<label>Current Time</label>
		</attribute>
		<attribute name='playFromCache' type='maya.bool'>
			<label>Play From Cache</label>
		</attribute>
		<attribute name='pointLight' type='maya.float3'>
			<label>Point Light</label>
		</attribute>
		<attribute name='textureOrigin' type='maya.float3'>
			<label>Texture Origin</label>
		</attribute>
		<attribute name='textureRotate' type='maya.float3'>
			<label>Texture Rotate</label>
		</attribute>
		<attribute name='textureScale' type='maya.float3'>
			<label>Texture Scale</label>
		</attribute>
		<attribute name='textureTime' type='maya.float'>
			<label>Texture Time</label>
		</attribute>
		<attribute name='visibility' type='maya.bool'>
			<label>Visibility</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEfluidShape'>
		<property name='matrix'/>
		<property name='inverseMatrix'/>
		<property name='worldMatrix'/>
		<property name='worldInverseMatrix'/>
		<property name='parentMatrix'/>
		<property name='parentInverseMatrix'/>
		<property name='outColor'/>
		<property name='outMatteOpacity'/>
		<property name='outTransparency'/>
		<property name='outMesh'/>
		<property name='currentTime'/>
		<property name='playFromCache'/>
		<property name='pointLight'/>
		<property name='textureOrigin'/>
		<property name='textureRotate'/>
		<property name='textureScale'/>
		<property name='textureTime'/>
		<property name='visibility'/>
	</view>
</templates>