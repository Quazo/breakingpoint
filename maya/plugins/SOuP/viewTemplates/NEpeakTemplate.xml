<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEpeak'>
		<attribute name='outputGeometry' type='maya.geometry'>
			<label>Output Geometry</label>
		</attribute>
		<attribute name='input' type='maya.TdataCompound'>
			<label>Input</label>
		</attribute>
		<attribute name='weightList' type='maya.TdataCompound'>
			<label>Weight List</label>
		</attribute>
		<attribute name='inComponents' type='maya.Int32Array'>
			<label>In Components</label>
		</attribute>
		<attribute name='inNormalPP' type='maya.vectorArray'>
			<label>In Normal PP</label>
		</attribute>
		<attribute name='inVelocityPP' type='maya.doubleArray'>
			<label>In Velocity PP</label>
		</attribute>
		<attribute name='inWeightPP' type='maya.doubleArray'>
			<label>In Weight PP</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEpeak'>
		<property name='outputGeometry'/>
		<property name='input'/>
		<property name='weightList'/>
		<property name='inComponents'/>
		<property name='inNormalPP'/>
		<property name='inVelocityPP'/>
		<property name='inWeightPP'/>
	</view>
</templates>