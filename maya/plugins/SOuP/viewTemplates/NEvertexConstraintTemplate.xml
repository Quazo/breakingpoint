<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEvertexConstraint'>
		<attribute name='outTranslate' type='maya.TdataCompound'>
			<label>Out Translate</label>
		</attribute>
		<attribute name='outRotate' type='maya.TdataCompound'>
			<label>Out Rotate</label>
		</attribute>
		<attribute name='inMesh' type='maya.mesh'>
			<label>In Mesh</label>
		</attribute>
		<attribute name='inParentWorldMatrix' type='maya.matrix'>
			<label>In Parent World Matrix</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEvertexConstraint'>
		<property name='outTranslate'/>
		<property name='outRotate'/>
		<property name='inMesh'/>
		<property name='inParentWorldMatrix'/>
	</view>
</templates>