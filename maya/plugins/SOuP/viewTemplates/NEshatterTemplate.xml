<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEshatter'>
		<attribute name='outGeometry' type='maya.mesh'>
			<label>Out Geometry</label>
		</attribute>
		<attribute name='inGeometry' type='maya.mesh'>
			<label>In Geometry</label>
		</attribute>
		<attribute name='inPositionPP' type='maya.nurbsCurve'>
			<label>In Position PP</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEshatter'>
		<property name='outGeometry'/>
		<property name='inGeometry'/>
		<property name='inPositionPP'/>
	</view>
</templates>