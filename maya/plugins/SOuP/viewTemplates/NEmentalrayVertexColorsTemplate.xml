<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEmentalrayVertexColors'>
		<attribute name='cpvSets' type='maya.string'>
			<label>Cpv Sets</label>
		</attribute>
		<attribute name='alphaGain' type='maya.float'>
			<label>Alpha Gain</label>
		</attribute>
		<attribute name='alphaIsLuminance' type='maya.bool'>
			<label>Alpha Is Luminance</label>
		</attribute>
		<attribute name='alphaOffset' type='maya.float'>
			<label>Alpha Offset</label>
		</attribute>
		<attribute name='colorGain' type='maya.float3'>
			<label>Color Gain</label>
		</attribute>
		<attribute name='colorOffset' type='maya.float3'>
			<label>Color Offset</label>
		</attribute>
		<attribute name='defaultColor' type='maya.float3'>
			<label>Default Color</label>
		</attribute>
		<attribute name='invert' type='maya.bool'>
			<label>Invert</label>
		</attribute>
		<attribute name='message' type='maya.message'>
			<label>Message</label>
		</attribute>
		<attribute name='outAlpha' type='maya.float'>
			<label>Out Alpha</label>
		</attribute>
		<attribute name='outColor' type='maya.float3'>
			<label>Out Color</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEmentalrayVertexColors'>
		<property name='cpvSets'/>
		<property name='alphaGain'/>
		<property name='alphaIsLuminance'/>
		<property name='alphaOffset'/>
		<property name='colorGain'/>
		<property name='colorOffset'/>
		<property name='defaultColor'/>
		<property name='invert'/>
		<property name='message'/>
		<property name='outAlpha'/>
		<property name='outColor'/>
	</view>
</templates>