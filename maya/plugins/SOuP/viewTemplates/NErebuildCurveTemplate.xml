<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NErebuildCurve'>
		<attribute name='outputCurve' type='maya.nurbsCurve'>
			<label>Output Curve</label>
		</attribute>
		<attribute name='inputCurve' type='maya.nurbsCurve'>
			<label>Input Curve</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NErebuildCurve'>
		<property name='outputCurve'/>
		<property name='inputCurve'/>
	</view>
</templates>