<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEbound'>
		<attribute name='outGeometry' type='maya.mesh'>
			<label>Out Geometry</label>
		</attribute>
		<attribute name='outPositionPP' type='maya.vectorArray'>
			<label>Out Position PP</label>
		</attribute>
		<attribute name='outCenter' type='maya.double3'>
			<label>Out Center</label>
		</attribute>
		<attribute name='outScale' type='maya.double3'>
			<label>Out Scale</label>
		</attribute>
		<attribute name='inGeometry' type='maya.pointArray'>
			<label>In Geometry</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEbound'>
		<property name='outGeometry'/>
		<property name='outPositionPP'/>
		<property name='outCenter'/>
		<property name='outScale'/>
		<property name='inGeometry'/>
	</view>
</templates>