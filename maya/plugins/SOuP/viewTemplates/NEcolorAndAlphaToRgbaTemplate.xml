<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<template name='NEcolorAndAlphaToRgba'>
		<attribute name='outRgbaPP' type='maya.doubleArray'>
			<label>Out Rgba PP</label>
		</attribute>
		<attribute name='inAlphaPP' type='maya.doubleArray'>
			<label>In Alpha PP</label>
		</attribute>
		<attribute name='inRgbPP' type='maya.vectorArray'>
			<label>In Rgb PP</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEcolorAndAlphaToRgba'>
		<property name='outRgbaPP'/>
		<property name='inAlphaPP'/>
		<property name='inRgbPP'/>
	</view>
</templates>