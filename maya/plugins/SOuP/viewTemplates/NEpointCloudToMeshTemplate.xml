<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEpointCloudToMesh'>
		<attribute name='outMesh' type='maya.mesh'>
			<label>Out Mesh</label>
		</attribute>
		<attribute name='inMapPP' type='maya.vectorArray'>
			<label>In Map PP</label>
		</attribute>
		<attribute name='inNormalPP' type='maya.vectorArray'>
			<label>In Normal PP</label>
		</attribute>
		<attribute name='inPositionPP' type='maya.vectorArray'>
			<label>In Position PP</label>
		</attribute>
		<attribute name='inRgbaPP' type='maya.vectorArray'>
			<label>In Rgba PP</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEpointCloudToMesh'>
		<property name='outMesh'/>
		<property name='inMapPP'/>
		<property name='inNormalPP'/>
		<property name='inPositionPP'/>
		<property name='inRgbaPP'/>
	</view>
</templates>