<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEdeleteComponent'>
		<attribute name='inputGeometry' type='maya.geometry'>
			<label>Input Geometry</label>
		</attribute>
		<attribute name='outputGeometry' type='maya.geometry'>
			<label>Output Geometry</label>
		</attribute>
		<attribute name='deleteComponents' type='maya.componentList'>
			<label>Delete Components</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEdeleteComponent'>
		<property name='inputGeometry'/>
		<property name='outputGeometry'/>
		<property name='deleteComponents'/>
	</view>
</templates>