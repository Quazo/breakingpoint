<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEconvexHull'>
		<attribute name='outMesh' type='maya.mesh'>
			<label>Out Mesh</label>
		</attribute>
		<attribute name='inGeometry' type='maya.vectorArray'>
			<label>In Geometry</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEconvexHull'>
		<property name='outMesh'/>
		<property name='inGeometry'/>
	</view>
</templates>