<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEarrayToPointColor'>
		<attribute name='outGeometry' type='maya.mesh'>
			<label>Out Geometry</label>
		</attribute>
		<attribute name='inGeometry' type='maya.mesh'>
			<label>In Geometry</label>
		</attribute>
		<attribute name='inComponents' type='maya.componentList'>
			<label>In Components</label>
		</attribute>
		<attribute name='inRgbaPP' type='maya.pointArray'>
			<label>In Rgba PP</label>
		</attribute>
		<attribute name='groupId' type='maya.long'>
			<label>Group Id</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEarrayToPointColor'>
		<property name='outGeometry'/>
		<property name='inGeometry'/>
		<property name='inComponents'/>
		<property name='inRgbaPP'/>
		<property name='groupId'/>
	</view>
</templates>