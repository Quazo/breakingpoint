<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEmesh2arrays'>
		<attribute name='positionArray' type='maya.vectorArray'>
			<label>Position Array</label>
		</attribute>
		<attribute name='normalArray' type='maya.vectorArray'>
			<label>Normal Array</label>
		</attribute>
		<attribute name='colorArray' type='maya.vectorArray'>
			<label>Color Array</label>
		</attribute>
		<attribute name='randomFloat' type='maya.doubleArray'>
			<label>Random Float</label>
		</attribute>
		<attribute name='randomVector' type='maya.vectorArray'>
			<label>Random Vector</label>
		</attribute>
		<attribute name='inputMesh' type='maya.mesh'>
			<label>Input Mesh</label>
		</attribute>
		<attribute name='originalMesh' type='maya.mesh'>
			<label>Original Mesh</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEmesh2arrays'>
		<property name='positionArray'/>
		<property name='normalArray'/>
		<property name='colorArray'/>
		<property name='randomFloat'/>
		<property name='randomVector'/>
		<property name='inputMesh'/>
		<property name='originalMesh'/>
	</view>
</templates>