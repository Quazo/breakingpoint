<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEcage'>
		<attribute name='outComponents' type='maya.componentList'>
			<label>Out Components</label>
		</attribute>
		<attribute name='inMesh' type='maya.mesh'>
			<label>In Mesh</label>
		</attribute>
		<attribute name='inMeshCage' type='maya.mesh'>
			<label>In Mesh Cage</label>
		</attribute>
		<attribute name='outVertComponents' type='maya.componentList'>
			<label>Out Vert Components</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEcage'>
		<property name='outComponents'/>
		<property name='inMesh'/>
		<property name='inMeshCage'/>
		<property name='outVertComponents'/>
	</view>
</templates>