<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEcape'>
		<attribute name='outputGeometry' type='maya.geometry'>
			<label>Output Geometry</label>
		</attribute>
		<attribute name='input' type='maya.TdataCompound'>
			<label>Input</label>
		</attribute>
		<attribute name='inCageMesh' type='maya.mesh'>
			<label>In Cage Mesh</label>
		</attribute>
		<attribute name='baseCageMesh' type='maya.mesh'>
			<label>Base Cage Mesh</label>
		</attribute>
		<attribute name='inCageCreasePP' type='maya.doubleArray'>
			<label>In Cage Crease PP</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEcape'>
		<property name='outputGeometry'/>
		<property name='input'/>
		<property name='inCageMesh'/>
		<property name='baseCageMesh'/>
		<property name='inCageCreasePP'/>
	</view>
</templates>