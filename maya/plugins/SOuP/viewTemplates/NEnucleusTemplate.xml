<?xml version='1.0' encoding='UTF-8'?>
<templates>
	<using package='maya'/>
	<using package='std'/>
	<template name='NEnucleus'>
		<attribute name='currentTime' type='maya.time'>
			<label>Current Time</label>
		</attribute>
		<attribute name='inputActive' type='maya.Nobject'>
			<label>Input Active</label>
		</attribute>
		<attribute name='inputActiveStart' type='maya.Nobject'>
			<label>Input Active Start</label>
		</attribute>
		<attribute name='inputPassive' type='maya.Nobject'>
			<label>Input Passive</label>
		</attribute>
		<attribute name='inputPassiveStart' type='maya.Nobject'>
			<label>Input Passive Start</label>
		</attribute>
		<attribute name='inverseMatrix' type='maya.matrix'>
			<label>Inverse Matrix</label>
		</attribute>
		<attribute name='matrix' type='maya.matrix'>
			<label>Matrix</label>
		</attribute>
		<attribute name='outputObjects' type='maya.Nobject'>
			<label>Output Objects</label>
		</attribute>
		<attribute name='parentInverseMatrix' type='maya.matrix'>
			<label>Parent Inverse Matrix</label>
		</attribute>
		<attribute name='parentMatrix' type='maya.matrix'>
			<label>Parent Matrix</label>
		</attribute>
		<attribute name='rotate' type='maya.double3'>
			<label>Rotate</label>
		</attribute>
		<attribute name='scale' type='maya.double3'>
			<label>Scale</label>
		</attribute>
		<attribute name='startFrame' type='maya.double'>
			<label>Start Frame</label>
		</attribute>
		<attribute name='translate' type='maya.double3'>
			<label>Translate</label>
		</attribute>
		<attribute name='visibility' type='maya.bool'>
			<label>Visibility</label>
		</attribute>
		<attribute name='windDirection' type='maya.float3'>
			<label>Wind Direction</label>
		</attribute>
		<attribute name='worldInverseMatrix' type='maya.matrix'>
			<label>World Inverse Matrix</label>
		</attribute>
		<attribute name='worldMatrix' type='maya.matrix'>
			<label>World Matrix</label>
		</attribute>
	</template>
	<view name='NEDefault' template='NEnucleus'>
		<property name='currentTime'/>
		<property name='inputActive'/>
		<property name='inputActiveStart'/>
		<property name='inputPassive'/>
		<property name='inputPassiveStart'/>
		<property name='inverseMatrix'/>
		<property name='matrix'/>
		<property name='outputObjects'/>
		<property name='parentInverseMatrix'/>
		<property name='parentMatrix'/>
		<property name='rotate'/>
		<property name='scale'/>
		<property name='startFrame'/>
		<property name='translate'/>
		<property name='visibility'/>
		<property name='windDirection'/>
		<property name='worldInverseMatrix'/>
		<property name='worldMatrix'/>
	</view>
</templates>