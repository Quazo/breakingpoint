[node density]
houdini.icon STRING "SOP_volume"
houdini.shader_type STRING "volume"
houdini.category STRING "Volume"
houdini.help_url STRING "https://support.solidangle.com/x/UwA7"

[attr scatter_channel]
desc STRING "The volume channel containing the scattering data. Scattering is the rate at which light is scattered (or reflected) at a given point. The greater the rate of scattering, the shorter the average distance a ray of light will travel through a volume before being bounced off of its course. Higher values make a harder, harsher reflective volume."
linkable BOOL false

[attr scatter_color]
desc STRING "A multiplier for the scatterring data."

[attr scatter_g]
min FLOAT -1
max FLOAT 1
linkable BOOL false
houdini.label STRING "Anisotropy"
desc STRING "Henyey-Greenstein Anisotropy coefficient between -1 (full back-scatter) and 1 (full forward-scatter). Default is 0 for an isotropic medium."

[attr absorption_channel]
desc STRING "The volume channel containing the absorption data. Absorption is the rate at which the intensity of a beam of light traversing a volume is lost due to either scattering and/or absorption events. Higher values make a harder, harsher, and darker volume."
linkable BOOL false

[attr absorption_color]
desc STRING "A multiplier for the absorption data."

[attr emission_channel]
desc STRING "The volume channel containing the emission data. Emission is the rate at which a volume emits light at a given point. The light emitted by a volume is visible to GI. It also will be affected by any attenuation, out-scattering or absorption effects in the volume."
linkable BOOL false

[attr emission_color]
desc STRING "A multiplier for the emission data."

[attr interpolation]
desc STRING "The interpolation to use when sampling volume data."
linkable BOOL false

