[node hair]
houdini.icon STRING "SHELF_hair"
houdini.category STRING "Shading"
houdini.parm.folder.folder1 STRING "Diffuse;6;Specular;9;Transmission;3;UV;2;AOVs;4"
houdini.parm.heading.heading2 STRING "Secondary Specular"
houdini.order STRING "folder1 rootcolor tipcolor opacity ambdiff kd_ind diffuse_cache spec_color spec gloss spec_shift heading2 spec2_color spec2 gloss2 spec2_shift transmission_color transmission transmission_spread uparam vparam aov_direct_diffuse aov_direct_specular aov_direct_transmission aov_indirect_diffuse"
houdini.help_url STRING "https://support.solidangle.com/x/ZoAo"

[attr rootcolor]
houdini.label STRING "Root Color"
desc STRING "The diffuse color at the root of the hair."

[attr tipcolor]
houdini.label STRING "Tip Color"
desc STRING "The diffuse color at the tip of the hair."

[attr opacity]
desc STRING "Controls how transparent the hair is. White is opaque and black is fully transparent. The opacity can be mapped along the hair strand using a hair opacity texture map."

[attr ambdiff]
houdini.label STRING "Ambient Diffuse"
desc STRING "Blends between two shading modes. 0 = diffuse will be fully isotropic. 1 = diffuse will follow the Kajiya-Kay diffuse equation. A value between 0 and 1 will blend between the two modes."

[attr spec]
houdini.label STRING "Specular"
desc STRING "Scale the specular contribution. The specular component represents light that is reflected off the outer surface of the hair strand."

[attr spec_color]
houdini.label STRING "Color"
desc STRING "The primary specular color."

[attr spec_shift]
houdini.label STRING "Shift"
softmin FLOAT -15
softmax FLOAT 15
desc STRING "Controls the offset of the specular highlight along the tube. An angular value that controls (in degrees) the shift of the specular lobes away from the perfect mirror direction."

[attr gloss]
houdini.label STRING "Glossiness"
softmin FLOAT 0
softmax FLOAT 20
desc STRING "Scales the glossy contribution. Higher values give a sleek and shiny look to the hair. Lower values give a dull matte appearance."

[attr spec2]
houdini.label STRING "Specular"
desc STRING "Scale the specular contribution. The specular component represents light that is reflected off the outer surface of the hair strand."

[attr spec2_color]
houdini.label STRING "Color"
desc STRING "The secondary specular color."

[attr spec2_shift]
houdini.label STRING "Shift"
softmin FLOAT -15
softmax FLOAT 15
desc STRING "Controls the offset of the specular highlight along the tube. An angular value that controls (in degrees) the shift of the specular lobes away from the perfect mirror direction."

[attr gloss2]
houdini.label STRING "Glossiness"
softmin FLOAT 0
softmax FLOAT 20
desc STRING "Scales the glossy contribution. Higher values give a sleek and shiny look to the hair. Lower values give a dull matte appearance."

[attr transmission]
desc STRING "Marschner-style transmission component scale, also known as TT. Transmission can be turned on and off per light with light.affect_specular."

[attr transmission_color]
houdini.label STRING "Color"
desc STRING "The transmission color."

[attr transmission_spread]
houdini.label STRING "Spread"
desc STRING "A smaller spread will correspond to more focused brighter effect. Default spread is 1.0, and reasonable values are in the 0.5 to 5.0 range."
softmin FLOAT 0.5
softmax FLOAT 5.0

[attr kd_ind]
houdini.label STRING "Indirect Diffuse"
desc STRING "Controls the amount of indirect light (GI light rays) the shader will receive."

[attr uparam]
houdini.label STRING "U Parameter"
desc STRING "The name of the float user data that will be used to compute the U of an alternative set of UV coordinates."

[attr vparam]
houdini.label STRING "V Parameter"
desc STRING "The name of the float user data that will be used to compute the V of an alternative set of UV coordinates."

[attr aov_direct_diffuse]
houdini.label STRING "Direct Diffuse"
linkable BOOL false
desc STRING "The name of the Direct Diffuse AOV."

[attr aov_direct_specular]
houdini.label STRING "Direct Specular"
linkable BOOL false
desc STRING "The name of the Direct Specular AOV."

[attr aov_direct_transmission]
houdini.label STRING "Direct Transmission"
linkable BOOL false
desc STRING "The name of the Direct Transmission AOV."

[attr aov_indirect_diffuse]
houdini.label STRING "Indirect Diffuse"
linkable BOOL false
desc STRING "The name of the Indirect Diffuse AOV."

[attr diffuse_cache]
desc STRING "It enables the computation of the direct diffuse lighting into each control point of each hair strand. Indirect diffuse rays will sample this baked illumination on the hair rather than the full hair shader network as it normally would. This can give a big reduction in render time if there are a lot of global illumination bounces in the scene."

