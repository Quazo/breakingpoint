[node volume_scattering]
houdini.label STRING "Atmospheric Scattering"
houdini.icon STRING "SHOP_fog"
houdini.shader_type STRING "atmosphere"
houdini.category STRING "Atmosphere"
houdini.order STRING "rgb_density density rgb_attenuation attenuation eccentricity samples affect_camera affect_diffuse affect_reflection"
houdini.help_url STRING "https://support.solidangle.com/x/0gAw"

[attr density]
desc STRING "Controls the thickness of the atmosphere."

[attr samples]
min FLOAT 1
softmax FLOAT 10
linkable BOOL false
desc STRING "Controls the number of samples used during atmospheric scattering. Increase this value if you are getting too much noise."

[attr eccentricity]
min FLOAT -1
max FLOAT 1
desc STRING "This control alters the directional bias, or anisotropy, of the scattering. A value of zero (the default) gives isotropic scattering (the light is scattered evenly in all directions, giving a uniform effect). Positive values bias the scattering effect forwards, in the direction of the light, while negative values bias the scattering backwards, toward the light."

[attr attenuation]
desc STRING "The attenuation parameter sets the rate at which the rays of light traveling through the scattering medium are extinguished. A high value means that light will only travel a short distance through the volume, while a low value means that light will travel a long distance through the volume."

[attr affect_camera]
desc STRING "Controls the degree to which atmospheric scattering affects camera rays."

[attr affect_diffuse]
desc STRING "Controls the degree to which atmospheric scattering affects diffuse GI rays."

[attr affect_reflection]
desc STRING "Controls the degree to which atmospheric scattering affects reflection / glossy rays."

[attr importance_sampling]
houdini.invisible BOOL true
houdini.no_input BOOL true
desc STRING "Enable importance sampling. This parameter should always be on, and thus is not exposed."

[attr rgb_density]
houdini.label STRING "Density Color"
desc STRING "RGB multiplier for the density parameter."

[attr rgb_attenuation]
houdini.label STRING "Attenuation Color"
desc STRING "RGB multiplier for the attenuation parameter."

