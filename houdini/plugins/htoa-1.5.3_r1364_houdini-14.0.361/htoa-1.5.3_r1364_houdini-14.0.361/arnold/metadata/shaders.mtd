[node image]
houdini.icon STRING "VOP_texture"
houdini.category STRING "Texture"
houdini.parm.folder.folder1 STRING "Texture;9; UV ;9"
houdini.order STRING "folder1 "
"filename multiply offset filter mipmap_bias single_channel start_channel ignore_missing_tiles missing_tile_color "
"uvcoords uvset sscale tscale swrap twrap swap_st sflip tflip"
houdini.help_url STRING "https://support.solidangle.com/x/Q4Ao"

[attr filename]
houdini.type STRING "file:image"
desc STRING "The path of the current location of the file texture map. The full path name must be used."

[attr filter]
desc STRING "Texel interpolation method used to filter the rendered image file. Cubic is slower but smoother, closest is faster but looks blocky when magnified."

[attr mipmap_bias]
softmin FLOAT -5
softmax FLOAT 5
linkable BOOL false
desc STRING "Offsets the Mip-Map level from which a texture is sampled. Negative value indicates a larger Mip-Map level (bigger texture) resulting in sharper texture lookups; a positive value indicates a smaller Mip-Map level (smaller texture) which will produce a blurrier result."

[attr multiply]
desc STRING "Multiplies the image by a constant. Useful for quickly adding a small amount of brightness."
linkable BOOL false

[attr offset]
desc STRING "Adds a constant to the image. Useful for quickly lifting the black level."

[attr single_channel]
desc STRING "Use only a single channel of the texture. The values on the channels other than the first is undefined."

[attr start_channel]
min FLOAT 0
softmax FLOAT 3
houdini.disable_when STRING "{ single_channel == 0 }"
desc STRING "When only a single channel is used, this defines an offset to the channel to read."

[attr swrap]
houdini.label STRING "Wrap U"
desc STRING "Controls how a texture repeats on a large surface."

[attr twrap]
houdini.label STRING "Wrap V"
desc STRING "Controls how a texture repeats on a large surface."

[attr sscale]
houdini.label STRING "Scale U"
softmin FLOAT 0
softmax FLOAT 2
desc STRING "Scales the size of the texture"

[attr tscale]
houdini.label STRING "Scale V"
softmin FLOAT 0
softmax FLOAT 2
desc STRING "Scales the size of the texture"

[attr sflip]
houdini.label STRING "Flip U"
desc STRING "Flips the texture in the U direction."

[attr tflip]
houdini.label STRING "Flip V"
desc STRING "Flips the texture in the V direction."

[attr swap_st]
houdini.label STRING "Swap U & V"

[attr uvcoords]
houdini.label STRING "Custom UVs"
desc STRING "When this parameter is linked, its input will be used to lookup "
"the texture instead of the object's UVs.\n\nWARNING: By specifying custom UV "
"coordinates in this shader parameter, Arnold will sadly stop providing "
"texture derivatives, which will absolutely destroy texture I/O performance: "
"there won't be any mip-mapping at all."

[attr uvset]
houdini.label STRING "UV Set"
houdini.type STRING "menu:replace:menu"
houdini.menu STRING ";Default\t(uv);uv2;UV Set 2\tuv2;uv3;UV Set 3\tuv3;uv4;UV Set 4\tuv4;uv5;UV Set 5\tuv5;uv6;UV Set 6\tuv6;uv7;UV Set 7\tuv7;uv8;UV Set 8\tuv8;uv9;UV Set 9\tuv9;uv10;UV Set 10   \tuv10"
desc STRING "An alternate UV set. It must be a float vertex attribute of size 2 and must be set for export in the Arnold properties of an object. If left blank, the standard uv attribute will be used."

[attr cache_texture_handles]
houdini.skip BOOL true

[attr ignore_missing_tiles]
desc STRING "Do not generate an error when a missing UDIM tile is encountered and display the missing_tile_color instead."

[attr missing_tile_color]
houdini.disable_when STRING "{ ignore_missing_tiles == 0 }"
desc STRING "The color to be displayed when a missing UDIM tile is encountered and ignore_missing_tiles is set."

