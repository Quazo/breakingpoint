[node gobo]
houdini.icon STRING "OBJ_hlight"
houdini.shader_type STRING "light_filter"
houdini.category STRING "Light Filter"
houdini.help_url STRING "https://support.solidangle.com/x/tAAw"

[attr rotate]
houdini.skip BOOL true
linkable BOOL false

[attr slidemap]
desc STRING "Texture map used to create the gobo effect."

[attr offset]
desc STRING "Offsets the position of the slide map. U Offset offsets the texture pattern along the U direction and V Offset along the V direction."
linkable BOOL false

[attr density]
desc STRING "Controls the density of the gobo. Higher values will make the gobo more opaque, thereby letting less light through."
linkable BOOL false

[attr filter_mode]
desc STRING "The blending equation by which the gobo's slide map is combined with the lights output.\nBlend: creates a default gobo effect.\nReplace: multiplies the slide map over the spot light.\nAdd: multiplies the spot light over the gobo.\nSub: inverts the RGB values of the slide map.\nMix: averages the results equally between the spotlight and the slide map."
linkable BOOL false

[attr scale_s]
desc STRING "Scales the size of the texture used for the slide map in the S direction."
linkable BOOL false

[attr scale_t]
desc STRING "Scales the size of the texture used for the slide map in the T direction."
linkable BOOL false

[attr wrap_s]
desc STRING "Controls how the 2d texture map is repeated on a surface in the S direction."
linkable BOOL false

[attr wrap_t]
desc STRING "Controls how the 2d texture map is repeated on a surface in the T direction."
linkable BOOL false

